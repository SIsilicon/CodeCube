; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "ProgramBlock",
"class": "ActionBlock",
"language": "GDScript",
"path": "res://program blocks/Blocks/Action Blocks/Action Block.gd"
}, {
"base": "Node",
"class": "FSM",
"language": "GDScript",
"path": "res://addons/node_fsm_plugin/fsm_node.gd"
}, {
"base": "Panel",
"class": "ProgramBlock",
"language": "GDScript",
"path": "res://program blocks/ProgramBlock.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://addons/node_fsm_plugin/state_node.gd"
}, {
"base": "StaticBody",
"class": "Tile",
"language": "GDScript",
"path": "res://tiles/Tile.gd"
} ]
_global_script_class_icons={
"ActionBlock": "",
"FSM": "res://addons/node_fsm_plugin/fsm_icon.svg",
"ProgramBlock": "",
"State": "res://addons/node_fsm_plugin/state_icon.svg",
"Tile": ""
}

[application]

config/name="Code Cube"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Global.gd"

[debug]

settings/fps/force_fps=30

[display]

window/size/resizable=false
window/size/test_width=1024
window/size/test_height=600
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "node_fsm_plugin" )

[gui]

theme/use_hidpi=true
theme/custom_font="res://fonts/Noto Sans UI/NotoSansUI.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

